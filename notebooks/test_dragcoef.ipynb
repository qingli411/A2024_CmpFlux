   "source": [
    "z0b = 0.01\n",
    "kappa = 0.4\n",
    "h = 0.2\n",
    "cd1 = kappa**2/np.log(h/2/z0b)**2\n",
    "cd2 = kappa**2/np.log((h/2+z0b)/z0b)**2\n",
    "cd3 = kappa**2/((h+z0b)/h*np.log((h+z0b)/z0b)-1)\n",
    "print(cd1)\n",
    "print(cd2)\n",
    "print(cd3)"
   ]
